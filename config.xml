<?xml version='1.0' encoding='UTF-8'?>
<widget xmlns="http://wirecloud.conwet.fi.upm.es/ns/macdescription/1" vendor="CRQGestionSL" name="restsql" version="0.1">
  <details>
    <title>RESTSQL</title>
    <authors>cquijano</authors>
		<email>cquijano@crqgestion.es</email>
    <image>img/catalogue.png</image>
		<description>Get Data from a REST/SQL Server</description>
  </details>
  <requirements>
    <feature name="NGSI"/>
  </requirements>
	<preferences>
  	<preference name="ngsi_server" type="text" label="NGSI Server" description="NGSI Server" default="http://130.206.127.187:1026/"/>
		<preference name="history_server" type="text" description="Service to get data from" label="history_server" default="http://130.206.127.187"/>
	</preferences>
	<wiring>
      <outputendpoint name="data_out" type="text" label="Data" friendcode="data" description="SQLDATA"/>
  </wiring>
  <contents src="index.html" contenttype="text/html" charset="utf-8" useplatformstyle="true"/>
  <rendering height="20" width="5"/>
</widget>
